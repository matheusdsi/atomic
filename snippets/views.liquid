{%- for block in section.blocks -%}
  {%- case block.type -%}
    {%- when 'views' -%}
    <div class="look">
      <p><span class="olho"></span>
      <b><span class="viwer_pro"></span> </b> {{ block.settings.titulo-views }}</p>
    </div>
    <script>
      $( ".viwer_pro" ).text(function(index) {
        return Math.floor(Math.random() * 50) + 1;
      });
    </script>
  {% endcase %}
{% endfor %}